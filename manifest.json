{
  "manifest_version": 2,
  "name": "Check blood sugar",
  "description": "Check blood sugar is chrome extension that will remind you to check blood sugar after every 2 hours.",
  "version": "1.0",
  "permissions": ["alarms"],
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "icons": {
    "128": "./assets/icon.png",
    "48": "./assets/icon.png",
    "16": "./assets/icon.png"
  },
  "browser_action": {
    "default_icon": "./assets/icon.png",
    "default_popup": "index.html"
  }
}
